<?xml version="1.0" encoding="UTF-8"?>
<Context>

    <!-- Database Configuration - JDBC -->
    <Parameter name="spring.datasource.url" value="${DB_NEXTSTEP_URL}"/>
    <Parameter name="spring.datasource.username" value="${DB_NEXTSTEP_USERNAME}"/>
    <Parameter name="spring.datasource.password" value="${DB_NEXTSTEP_PASSWORD}"/>
    <Parameter name="spring.datasource.driver-class-name" value="${DB_NEXTSTEP_DRIVER}"/>
    <Parameter name="spring.jpa.hibernate.ddl-auto" value="none"/>

</Context>
